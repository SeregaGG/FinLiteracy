<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameStackprofitone"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    style="@style/map_blured">

    <LinearLayout
        android:id="@+id/linearColumnprofitone"
        android:layout_gravity="top|start"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/_40pxh"
            app:contentInsetStart="0dp"
            style="@style/groupStylegray_300">

            <LinearLayout
                android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_42pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:baselineAligned="false">

                <LinearLayout
                    android:id="@+id/linearRowprofitone"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1.5">

                    <ImageView
                        android:id="@+id/imageProfitOne"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_profit"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_profit" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtScore"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.6"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_500"
                        style="@style/txtMontserratbold20_1"
                        tools:text="@string/lbl_500"
                        tools:ignore="NestedWeights" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowalarmclockof"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="right"
                    android:layout_weight="0.5">

                    <ImageView
                        android:id="@+id/imageAlarmclockof"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_10pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_alarmclockof"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_alarmclockof" />

                    <TextView
                        android:layout_gravity=""
                        android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_70pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_40_03"
                        style="@style/txtMontserratbold20_2"
                        tools:text="@string/lbl_40_03"
                        tools:ignore="NestedWeights" />
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <FrameLayout
            android:gravity="start"
            android:id="@+id/linearRowleftarrowtwo"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_30pxv">

            <ImageView
                android:id="@+id/imageLeftarrowTwo"
                android:layout_height="@dimen/_30pxh"
                android:layout_width="@dimen/_30pxh"
                android:onClick="returnToMap"
                android:layout_gravity="center_vertical"
                android:src="@drawable/img_left_arrow"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_left_arrow" />

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtLoc"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_gravity="center"
                android:singleLine="true"
                android:text="@string/lbl26"
                style="@style/txtMontserratbold40"
                tools:text="@string/lbl26" />
        </FrameLayout>

        <LinearLayout
            android:gravity="center_horizontal"
            android:id="@+id/linearColumnfive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylewhite_A700cornerRadius">

            <TextView
                android:gravity="center_horizontal"
                android:id="@+id/txtFive"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent"
                android:text="@string/msg7"
                style="@style/txtMontserratbold15"
                tools:text="@string/msg7" />

            <LinearLayout
                android:gravity="start"
                android:id="@+id/linearRow2202w020n001"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameStack2202w020n001"
                    android:layout_height="@dimen/_104pxv"
                    android:layout_width="@dimen/_120pxh">

                    <ImageButton
                        android:id="@+id/imagePacket"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_90pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:onClick="easyTask"
                        android:background="@null"
                        android:src="@drawable/packet"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/packet" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_29pxh"
                        android:layout_marginStart="@dimen/_29pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl27"
                        style="@style/easy_task"
                        tools:text="@string/lbl27" />
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameStack2202w020n001One"
                    android:layout_height="@dimen/_103pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_120pxh">

                    <ImageButton
                        android:id="@+id/imagePacketMiddle"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_90pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:onClick="middleTask"
                        android:scaleType="fitXY"
                        android:background="@null"
                        android:src="@drawable/packet"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/packet" />

                    <TextView
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtSix"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl28"
                        style="@style/middle_task"
                        tools:text="@string/lbl28" />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:baselineAligned="false">

                <LinearLayout
                    android:id="@+id/linearRowtwohundred"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtTwoHundred"
                        style="@style/txtMontserratbold15_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.62"
                        android:gravity="right"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_200"
                        tools:ignore="NestedWeights"
                        tools:text="@string/lbl_200" />

                    <ImageView
                        android:id="@+id/imageCoinsTwo"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_coins"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_coins" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowthreehundred"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_87pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:orientation="horizontal">

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtThreeHundred"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.62"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_300"
                        style="@style/txtMontserratbold15_1"
                        tools:text="@string/lbl_300" />

                    <ImageView
                        android:id="@+id/imageCoinsFive"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_coins"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_coins" />
                </LinearLayout>
            </LinearLayout>

            <FrameLayout
                android:id="@+id/frameStack2202w020n001Two"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_104pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_120pxh">

                <ImageButton
                    android:id="@+id/imagePacketHard"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_90pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:onClick="hardTask"
                    android:background="@null"
                    android:src="@drawable/packet"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/packet" />

                <TextView
                    android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtSeven"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl29"
                    style="@style/hard_task"
                    tools:text="@string/lbl29" />
            </FrameLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:id="@+id/linearRowfourhundred"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_25pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtFourHundred"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.63"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/lbl_400"
                    style="@style/txtMontserratbold15_1"
                    tools:text="@string/lbl_400" />

                <ImageView
                    android:id="@+id/imageCoinsFour"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_coins"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_coins" />
            </LinearLayout>
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/btn_bank"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv"
            android:singleLine="true"
            android:text="@string/lbl30"
            android:textAllCaps="false"
            style="@style/btnSolidRounded"
            tools:text="@string/lbl30" />
    </LinearLayout>
</FrameLayout>
