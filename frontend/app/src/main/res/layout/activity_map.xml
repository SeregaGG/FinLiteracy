<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearColumnprofitone"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/map_back">

    <androidx.appcompat.widget.Toolbar
        android:gravity="start"
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        app:contentInsetStart="0dp"
        style="@style/groupStylegray_300">

        <LinearLayout
            android:id="@+id/linearRow"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_59pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:baselineAligned="false">

            <LinearLayout
                android:id="@+id/linearRowprofitone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1.5">

                <ImageView
                    android:id="@+id/imageProfitOne"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_profit"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_profit" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtScore"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.6"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_500"
                    style="@style/txtMontserratbold20_1"
                    tools:text="@string/lbl_500"
                    tools:ignore="NestedWeights" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowalarmclockof"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="right"
                android:layout_weight="0.5">

                <ImageView
                    android:id="@+id/imageAlarmclockof"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_alarmclockof"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_alarmclockof" />

                <TextView
                    android:layout_gravity=""
                    android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_40_03"
                    style="@style/txtMontserratbold20_2"
                    tools:text="@string/lbl_40_03"
                    tools:ignore="NestedWeights" />
            </LinearLayout>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>


    <android.widget.Button
        android:id="@+id/btn_school"
        style="@style/btnSolidRoundedOutline_1"
        android:layout_width="@dimen/_120pxh"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_marginStart="@dimen/_240pxv"
        android:layout_marginTop="@dimen/_80pxv"
        android:layout_marginEnd="@dimen/_40pxv"
        android:ellipsize="end"
        android:gravity="center"
        android:onClick="schoolToTask"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_29pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl26"
        android:textAllCaps="false"
        tools:text="@string/lbl26" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_280pxv"
        android:layout_marginTop="@dimen/_2pxh"
        android:src="@drawable/minus_100"/>

    <android.widget.Button
        android:id="@+id/btn_mall"
        style="@style/not_passed_btn"
        android:layout_width="@dimen/_120pxh"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_marginTop="@dimen/_30pxv"
        android:layout_marginStart="@dimen/_260pxv"
        android:layout_marginEnd="@dimen/_10pxv"
        android:ellipsize="end"
        android:gravity="center"
        android:onClick="mallToTask"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl57"
        android:textAllCaps="false"
        tools:text="@string/lbl57" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_300pxv"
        android:layout_marginTop="@dimen/_2pxh"
        android:src="@drawable/minus_100"/>

    <android.widget.Button
        android:id="@+id/btn_shop"
        style="@style/not_passed_btn"
        android:layout_width="@dimen/_120pxh"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:ellipsize="end"
        android:layout_marginStart="@dimen/_15pxv"
        android:gravity="center"
        android:onClick="marketToTask"
        android:paddingStart="@dimen/_23pxh"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_21pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl58"
        android:textAllCaps="false"
        tools:text="@string/lbl58" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_55pxv"
        android:layout_marginTop="@dimen/_2pxh"
        android:src="@drawable/minus_100"/>

    <android.widget.Button
        android:id="@+id/btn_bank"
        style="@style/not_passed_btn"
        android:layout_width="@dimen/_120pxh"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_marginTop="@dimen/_60pxv"
        android:layout_marginStart="@dimen/_220pxh"
        android:ellipsize="end"
        android:gravity="center"
        android:onClick="atmToTask"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_15pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl59"
        android:textAllCaps="false"
        tools:text="@string/lbl59" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_300pxv"
        android:layout_marginTop="@dimen/_2pxh"
        android:src="@drawable/minus_100"/>

    <android.widget.Button
        android:id="@+id/btn_fin_org"
        style="@style/not_passed_btn"
        android:layout_width="@dimen/_120pxh"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_marginTop="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxv"
        android:ellipsize="end"
        android:gravity="center"
        android:onClick="bankToTask"
        android:paddingStart="@dimen/_30pxv"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl56"
        android:textAllCaps="false"
        tools:text="@string/lbl56" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_60pxv"
        android:layout_marginTop="@dimen/_2pxh"
        android:src="@drawable/minus_100"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_finish"
        style="@style/btnSolidRounded_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_60pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:ellipsize="end"
        android:gravity="center"
        android:onClick="clickFinishBtn"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_15pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_15pxv"
        android:singleLine="true"
        android:text="@string/lbl75"
        android:textAllCaps="false"
        android:visibility="invisible"
        tools:text="@string/lbl75" />

    <TextView
        android:ellipsize="end"
        android:id="@+id/txtGroupThirtyFive"
        android:layout_gravity="end"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_20pxv"
        android:layout_marginEnd="@dimen/_25pxh"
        android:layout_marginStart="@dimen/_25pxh"
        android:layout_marginTop="@dimen/_114pxv"
        android:layout_width="wrap_content"
        android:paddingBottom="@dimen/_5pxv"
        android:paddingEnd="@dimen/_11pxh"
        android:paddingStart="@dimen/_10pxh"
        android:paddingTop="@dimen/_5pxv"
        android:onClick="clickHelpButton"
        android:singleLine="true"
        android:text="@string/lbl60"
        style="@style/txtSolidRounded"
        tools:text="@string/lbl60" />
</LinearLayout>
