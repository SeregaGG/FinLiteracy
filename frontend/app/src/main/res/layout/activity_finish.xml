<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
    android:id="@+id/linearColumnprofitone"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/background">

    <androidx.appcompat.widget.Toolbar
        android:gravity="start"
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        app:contentInsetStart="0dp"
        style="@style/groupStylegray_300">

        <LinearLayout
            android:id="@+id/linearRow"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_59pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:baselineAligned="false">

            <LinearLayout
                android:id="@+id/linearRowprofitone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1.5">

                <ImageView
                    android:id="@+id/imageProfitOne"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_profit"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_profit" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtScore"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.6"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_500"
                    style="@style/txtMontserratbold20_1"
                    tools:text="@string/lbl_500"
                    tools:ignore="NestedWeights" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowalarmclockof"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="right"
                android:layout_weight="0.5">

                <ImageView
                    android:id="@+id/imageAlarmclockof"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_alarmclockof"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_alarmclockof" />

                <TextView
                    android:layout_gravity=""
                    android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_40_03"
                    style="@style/txtMontserratbold20_2"
                    tools:text="@string/lbl_40_03"
                    tools:ignore="NestedWeights" />
            </LinearLayout>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>

    <LinearLayout
        android:gravity="end"
        android:id="@+id/linearImage"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_48pxv"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700cornerRadius">

        <TextView
            android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtFortySix"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_36pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/lbl62"
            style="@style/txtMontserratbold30"
            tools:text="@string/lbl62" />

        <TextView
            android:ellipsize="end"
            android:id="@+id/txtThreeHundredOne"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:singleLine="true"
            android:text="@string/lbl_3002"
            style="@style/txtMontserratsemibold20"
            tools:text="@string/lbl_3002" />

        <ImageView
            android:id="@+id/imageChest"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_200pxh"
            android:layout_width="@dimen/_200pxh"
            android:scaleType="fitXY"
            android:src="@drawable/chest"
            tools:ignore="ContentDescription"
            tools:src="@drawable/chest" />

        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/txtFortySeven"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_36pxv"
            android:layout_marginEnd="@dimen/_35pxh"
            android:layout_marginStart="@dimen/_35pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:text="@string/msg25"
            style="@style/txtMontserratsemibold15"
            tools:text="@string/msg25" />
    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end"
        android:gravity="center"
        android:id="@+id/btn_bank"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_20pxv"
        android:layout_marginEnd="@dimen/_22pxh"
        android:layout_marginStart="@dimen/_22pxh"
        android:layout_marginTop="@dimen/_59pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_14pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_14pxv"
        android:singleLine="true"
        android:onClick="clickHelpButton"
        android:text="@string/lbl35"
        android:textAllCaps="false"
        style="@style/btnSolidRounded"
        tools:text="@string/lbl35" />
</LinearLayout>
