<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="finishVM" type="com.daniiltropolsapplication.app.modules.finish.data.viewmodel.FinishVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnprofitone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle_1">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylegray_300">
            <LinearLayout android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_25pxv"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowprofitone"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.44"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageProfitOne"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_profit1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_profit1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThreeHundred"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.6"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{finishVM.finishModel.txtThreeHundred}"
                        style="@style/txtMontserratbold20_1" tools:text="@string/lbl_300"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowalarmclockof"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_weight="0.56"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageAlarmclockof"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_alarmclockof"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_alarmclockof"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.69"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{finishVM.finishModel.txtTime}"
                        style="@style/txtMontserratbold20_2" tools:text="@string/lbl_00_00"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearImage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtFortySix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{finishVM.finishModel.txtFortySix}"
                style="@style/txtMontserratbold30" tools:text="@string/lbl62"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtThreeHundredOne"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_92pxh"
                android:layout_marginStart="@dimen/_68pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{finishVM.finishModel.txtThreeHundredOne}"
                style="@style/txtMontserratsemibold20" tools:text="@string/lbl_3002"/>
            <ImageView android:id="@+id/imageChest"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_200pxh"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_200pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_135211171}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_135211171"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtFortySeven"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_36pxv"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:text="@{finishVM.finishModel.txtFortySeven}"
                style="@style/txtMontserratsemibold15" tools:text="@string/msg25"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowfortyeight"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_118pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleorange_A200cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtFortyEight"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_100pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.75" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{finishVM.finishModel.txtFortyEight}"
                style="@style/txtMontserratsemibold22" tools:text="@string/lbl35"/>
            <ImageView android:id="@+id/imageNextOne"
                android:layout_height="@dimen/_27pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_96pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_27pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_next1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_next1"/>
        </LinearLayout>
    </LinearLayout>
</layout>
