<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameStacklanguage"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    style="@style/sky_back">

    <LinearLayout
        android:id="@+id/linearColumnlanguage"
        android:layout_gravity="center"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_17pxv"
        android:layout_marginEnd="@dimen/_10pxh"
        android:layout_marginStart="@dimen/_10pxh"
        android:layout_marginTop="@dimen/_27pxv"
        android:layout_width="match_parent"
        android:orientation="vertical">


        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:text="@string/msg"
            style="@style/txtRibeyeRUSBYLYAJKAribeyeregular33"
            tools:text="@string/msg" />

        <LinearLayout
            android:gravity="start"
            android:id="@+id/linearRowcomponenttwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_31pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:background="@drawable/rectangle_bg_gray_100_radius_35"
                android:id="@+id/btnComponentTwo"
                android:layout_height="@dimen/_70pxh"
                android:layout_marginBottom="@dimen/_40pxv"
                android:layout_marginTop="@dimen/_99pxv"
                android:layout_width="@dimen/_70pxh"
                android:paddingHorizontal="@dimen/_14pxh"
                android:paddingVertical="@dimen/_14pxv"
                android:scaleType="fitCenter"
                android:onClick="toHelp"
                android:src="@drawable/component" />

            <LinearLayout
                android:gravity="center_horizontal"
                android:id="@+id/linearColumncomponentthree"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_weight="0.46"
                android:layout_width="0dp"
                android:orientation="vertical">

                <ImageButton
                    android:gravity="start"
                    android:id="@+id/imagePlaybuttonTwo"
                    android:layout_height="@dimen/_120pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/play_button"
                    android:onClick="toDialogBegin"
                    android:background="@null"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/play_button" />

                <ImageButton
                    android:background="@drawable/rectangle_bg_gray_100_radius_35"
                    android:id="@+id/btnComponentThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_70pxh"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:paddingHorizontal="@dimen/_11pxh"
                    android:paddingVertical="@dimen/_11pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/component_2" />
            </LinearLayout>

            <ImageButton
                android:background="@drawable/rectangle_bg_gray_100_radius_35"
                android:id="@+id/btnComponentFour"
                android:layout_height="@dimen/_70pxh"
                android:layout_marginBottom="@dimen/_40pxv"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_99pxv"
                android:layout_width="@dimen/_70pxh"
                android:paddingHorizontal="@dimen/_14pxh"
                android:paddingVertical="@dimen/_14pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/component_3" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameStackbiruzafone"
            android:layout_gravity="start"
            android:layout_height="@dimen/_340pxv"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_width="@dimen/_340pxh">

            <ImageView
                android:id="@+id/imageBiruzafOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_340pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_biruzaf"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_biruzaf" />

            <LinearLayout
                android:id="@+id/linearColumnthirtysix"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageCall"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_46pxh"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_call"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_call" />

                <FrameLayout
                    android:id="@+id/frameStackthirtysix"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_65pxv"
                    android:layout_marginEnd="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_width="@dimen/_50pxh">

                    <TextView
                        android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtThirtySix"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl51"
                        style="@style/txtTkachenkoSketch4Fbold25"
                        tools:text="@string/lbl51" />

                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearRowthirtyseven"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:baselineAligned="false">

                    <LinearLayout
                        android:gravity="start"
                        android:id="@+id/linearColumnthirtyseven"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_weight="0.88"
                        android:layout_width="0dp"
                        android:orientation="vertical">

                        <TextView
                            android:ellipsize="end"
                            android:id="@+id/txtThirtySeven"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@string/lbl52"
                            style="@style/txtTkachenkoSketch4Fbold25_1"
                            tools:text="@string/lbl52" />

                        <ImageView
                            android:id="@+id/imageForward"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_forward"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_forward" />
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/frameStackthirtyeight"
                        android:layout_height="@dimen/_56pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_width="@dimen/_38pxh">

                        <TextView
                            android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtThirtyEight"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl53"
                            style="@style/txtTkachenkoSketch4Fbold25_2"
                            tools:text="@string/lbl53" />

                        <ImageView
                            android:id="@+id/imageLocation"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_location" />
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</FrameLayout>
