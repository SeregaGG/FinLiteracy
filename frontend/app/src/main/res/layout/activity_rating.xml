<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
    android:id="@+id/linearOne"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@drawable/blured_back">


    <LinearLayout
        android:gravity="start"
        android:id="@+id/linearColumntwentyeight"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical">

        <TextView
            android:ellipsize="end"
            android:id="@+id/txtTwentyEight"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl40"
            style="@style/txtMontserratbold15"
            tools:text="@string/lbl40" />

        <EditText
            android:background="@drawable/rectangle_bg_white_a700_radius_25"
            android:ellipsize="end"
            android:hint="@string/lbl21"
            android:id="@+id/etTableHead"
            android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_26pxh"
            android:paddingTop="@dimen/_19pxv"
            android:singleLine="true"
            android:textColorHint="@color/black_900_b2"
            style="@style/etSolidRounded_3"
            tools:ignore="Autofill" />

        <LinearLayout
            android:gravity="start"
            android:id="@+id/linearRowtwentynine"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtTwentyNine"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.53"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl41"
                style="@style/txtMontserratbold15"
                tools:text="@string/lbl41" />

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtThirty"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_166pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.47"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl42"
                style="@style/txtMontserratbold15"
                tools:text="@string/lbl42" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearRowgroup106"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <Spinner
                android:background="@drawable/layer_list_bg_white_a700_radius_25"
                android:id="@+id/spinnerGroup106"
                android:layout_height="wrap_content"
                android:layout_weight="0.68"
                android:layout_width="0dp"
                tools:listitem="@layout/spinner_item" />

            <Spinner
                android:background="@drawable/layer_list_bg_white_a700_radius_25"
                android:id="@+id/spinnerGroup105"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_weight="0.32"
                android:layout_width="0dp"
                tools:listitem="@layout/spinner_item" />
        </LinearLayout>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/frameStackthirtyone"
        android:layout_gravity="center_horizontal"
        android:layout_height="@dimen/_330pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_19pxv"
        android:layout_width="@dimen/_320pxh">

        <LinearLayout
            android:id="@+id/linearColumnone"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylewhite_A700cornerRadius">

            <LinearLayout
                android:gravity="center_horizontal"
                android:id="@+id/linearColumnthirtyone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                style="@style/groupStylebluegray_300cornerRadius">

                <TextView
                    android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtThirtyOne"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl43"
                    style="@style/txtMontserratbold18"
                    tools:text="@string/lbl43" />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerRating"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_24pxh}"
                tools:itemCount="4"
                tools:listitem="@layout/row_rating" />

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtThirtyTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl48"
                style="@style/txtMontserratsemibold15"
                tools:text="@string/lbl48" />

            <LinearLayout
                android:gravity="end"
                android:id="@+id/linearRowtwenty"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStylegray_300_cccornerRadius">

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtTwenty"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.1"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/lbl_202"
                    style="@style/txtMontserratsemibold15"
                    tools:text="@string/lbl_202" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtThirtyThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.77"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/msg18"
                    style="@style/txtMontserratmedium15"
                    tools:text="@string/msg18" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.05"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/lbl_0"
                    style="@style/txtMontserratbold15_1"
                    tools:text="@string/lbl_0" />

                <ImageView
                    android:id="@+id/imageCoinsSix"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_coins2"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_coins2" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineTen"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_57pxv"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_57pxv"
            android:layout_width="@dimen/_318pxh" />

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineEleven"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_102pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_102pxv"
            android:layout_width="@dimen/_318pxh" />

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineTwelve"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_147pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_147pxv"
            android:layout_width="@dimen/_318pxh" />

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineThirteen"
            android:layout_gravity="bottom|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_135pxv"
            android:layout_marginEnd="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_135pxv"
            android:layout_width="@dimen/_318pxh" />

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineFourteen"
            android:layout_gravity="bottom|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_90pxv"
            android:layout_marginEnd="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_90pxv"
            android:layout_width="@dimen/_318pxh" />

        <View
            android:background="@color/black_900_14"
            android:id="@+id/lineLineFifteen"
            android:layout_gravity="bottom|center"
            android:layout_height="@dimen/_3pxv"
            android:layout_marginBottom="@dimen/_45pxv"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_45pxv"
            android:layout_width="@dimen/_318pxh" />
    </FrameLayout>

    <TextView
        android:ellipsize="end"
        android:id="@+id/txtThirtyFour"
        android:layout_gravity="end"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_21pxh"
        android:layout_marginStart="@dimen/_21pxh"
        android:layout_marginTop="@dimen/_8pxv"
        android:layout_width="wrap_content"
        android:singleLine="true"
        android:text="@string/lbl49"
        style="@style/txtMontserratbold13"
        tools:text="@string/lbl49" />

    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/linearRowthirtyfive"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_34pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_17pxv"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        style="@style/groupStyleorange_A200cornerRadius">

        <TextView
            android:ellipsize="end"
            android:id="@+id/txtThirtyFive"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_weight="0.88"
            android:layout_width="0dp"
            android:singleLine="true"
            android:text="@string/lbl50"
            style="@style/txtMontserratsemibold22"
            tools:text="@string/lbl50" />

        <ImageView
            android:id="@+id/imageNextOne"
            android:layout_height="@dimen/_27pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_45pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_27pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_next1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_next1" />
    </LinearLayout>
</LinearLayout>
