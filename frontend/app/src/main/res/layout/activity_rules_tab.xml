<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameStackprofitone"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    style="@style/groupStyle_3">

    <LinearLayout
        android:id="@+id/linearColumnprofitone"
        android:layout_gravity="top|start"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="@drawable/blured_back">

        <androidx.appcompat.widget.Toolbar
            android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            style="@style/groupStylegray_300">

            <LinearLayout
                android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_42pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:baselineAligned="false">

                <LinearLayout
                    android:id="@+id/linearRowprofitone"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1.5">

                    <ImageView
                        android:id="@+id/imageProfitOne"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_profit"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_profit" />

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtScore"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.6"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_500"
                        style="@style/txtMontserratbold20_1"
                        tools:text="@string/lbl_500"
                        tools:ignore="NestedWeights" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearRowalarmclockof"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="right"
                    android:layout_weight="0.5">

                    <ImageView
                        android:id="@+id/imageAlarmclockof"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_width="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_10pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_alarmclockof"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_alarmclockof" />

                    <TextView
                        android:layout_gravity=""
                        android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_40_03"
                        style="@style/txtMontserratbold20_2"
                        tools:text="@string/lbl_40_03"
                        tools:ignore="NestedWeights" />
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:gravity="start"
            android:id="@+id/linearRowleftarrowfour"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_26pxh"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageLeftarrowFour"
                android:layout_height="@dimen/_30pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:onClick="returnFromHelp"
                android:src="@drawable/img_left_arrow"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_left_arrow" />

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtNinetyTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.88"
                android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl54"
                style="@style/txtMontserratbold24"
                tools:text="@string/lbl54" />
        </LinearLayout>

        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/txtNinetyThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:text="@string/msg49"
            style="@style/txtMontserratsemibold15_2"
            tools:text="@string/msg49" />

        <LinearLayout
            android:id="@+id/linearColumnhomeone"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_marginStart="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_width="match_parent"
            android:orientation="vertical"
            style="@style/groupStylewhite_A700cornerRadius">

            <LinearLayout
                android:gravity="start"
                android:id="@+id/linearRowhomeone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20pxv"
                style="@style/rules_tab">

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home" />

                <TextView
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:text="@string/msg50"
                    style="@style/txtMontserratsemibold13"
                    tools:text="@string/msg50" />
            </LinearLayout>

            <View android:background="@color/black_900_14"
                android:layout_height="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_15pxv"/>

            <LinearLayout
                android:gravity="start"
                android:id="@+id/linearRowcoinsone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/rules_tab"
                android:layout_marginTop="@dimen/_15pxv">

                <ImageView
                    android:id="@+id/imageCoinsOne"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_coins_2"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_coins_2" />

                <TextView
                    android:id="@+id/txtNinetyFour"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:text="@string/msg53"
                    style="@style/txtMontserratsemibold13"
                    tools:text="@string/msg53" />
            </LinearLayout>

            <View android:background="@color/black_900_14"
                android:layout_height="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_15pxv"/>

            <LinearLayout
                android:gravity="start"
                android:id="@+id/linearRowcheckingone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_15pxv"
                style="@style/rules_tab">

                <ImageView
                    android:id="@+id/imageCheckingOne"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_checking"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_checking" />

                <TextView
                    android:id="@+id/txtNinetyFive"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_weight="0.86"
                    android:layout_width="0dp"
                    android:text="@string/msg56"
                    style="@style/txtMontserratsemibold13"
                    tools:text="@string/msg56" />
            </LinearLayout>

            <View android:background="@color/black_900_14"
                android:layout_height="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_15pxv"/>

            <LinearLayout
                android:gravity="start"
                android:id="@+id/linearRowtrophyone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginBottom="@dimen/_20pxv"
                style="@style/rules_tab">

                <ImageView
                    android:id="@+id/imageTrophyOne"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_trophy"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_trophy" />

                <TextView
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:text="@string/msg60"
                    style="@style/txtMontserratsemibold13"
                    tools:text="@string/msg60" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:ellipsize="end"
            android:layout_gravity="center"
            android:id="@+id/txtNinetySix"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/msg61"
            style="@style/txtMontserratsemibold15_2"
            tools:text="@string/msg61" />
    </LinearLayout>
</FrameLayout>