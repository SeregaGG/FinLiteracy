<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
    android:id="@+id/linearColumnprofitone"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    style="@style/map_blured">

    <androidx.appcompat.widget.Toolbar
        android:gravity="start"
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/_40pxh"
        app:contentInsetStart="0dp"
        style="@style/groupStylegray_300">

        <LinearLayout
            android:id="@+id/linearRow"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_59pxh"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:baselineAligned="false">

            <LinearLayout
                android:id="@+id/linearRowprofitone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1.5">

                <ImageView
                    android:id="@+id/imageProfitOne"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_profit"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_profit" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtScore"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.6"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_500"
                    style="@style/txtMontserratbold20_1"
                    tools:text="@string/lbl_500"
                    tools:ignore="NestedWeights" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowalarmclockof"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="right"
                android:layout_weight="0.5">

                <ImageView
                    android:id="@+id/imageAlarmclockof"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_alarmclockof"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_alarmclockof" />

                <TextView
                    android:layout_gravity=""
                    android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_40_03"
                    style="@style/txtMontserratbold20_2"
                    tools:text="@string/lbl_40_03"
                    tools:ignore="NestedWeights" />
            </LinearLayout>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:ellipsize="end"
        android:id="@+id/txtLoc"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_22pxh"
        android:layout_marginStart="@dimen/_22pxh"
        android:layout_marginTop="@dimen/_30pxv"
        android:layout_width="wrap_content"
        android:singleLine="true"
        android:text="@string/lbl26"
        style="@style/txtMontserratbold40"
        tools:text="@string/lbl26" />

    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/linearRowfivehundredOne"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10pxv"
        android:layout_width="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:ellipsize="end"
            android:id="@+id/txtFiveHundredOne"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_5002"
            style="@style/txtMontserratbold18_1"
            tools:text="@string/lbl_5002" />

        <ImageView
            android:id="@+id/imageProfitTwo"
            android:layout_height="@dimen/_20pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_20pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_profit"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_profit" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearColumneightyseven"
        style="@style/groupStylewhite_A700cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_22pxh"
        android:layout_marginTop="@dimen/_30pxv"
        android:layout_marginEnd="@dimen/_22pxh"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageCheckOne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:scaleType="fitXY"
            android:src="@drawable/correct_answer_pic"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/txtLanguage"
            style="@style/txtMontserratsemibold15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginBottom="@dimen/_30pxv"
            android:gravity="center_horizontal"
            android:text="@string/msg45"
            tools:text="@string/msg45" />

    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end"
        android:gravity="center"
        android:id="@+id/btn_bank"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_20pxv"
        android:layout_marginEnd="@dimen/_22pxh"
        android:layout_marginStart="@dimen/_22pxh"
        android:layout_marginTop="@dimen/_30pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_14pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:onClick="returnToMap"
        android:paddingTop="@dimen/_14pxv"
        android:singleLine="true"
        android:text="@string/lbl35"
        android:textAllCaps="false"
        style="@style/btnSolidRounded"
        tools:text="@string/lbl35" />
</LinearLayout>